int main()
{
	printf("%d\n", sizeof(char *));     // 4
	printf("%d\n", sizeof(short *));    // 4
	printf("%d\n", sizeof(int *));      // 4
	printf("%d\n", sizeof(double *));   // 4
	system("pause");
	return 0;
}
