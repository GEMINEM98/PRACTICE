//模拟实现strcmp(比较)
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<assert.h>
#include<stdlib.h>

int my_strcmp(const char* string1, const char* string2)
{
	assert(string1&&string2);
	while (*string1&&*string2)
	{
		if (*(unsigned char*)string1 < *(unsigned char*)string2)
		{
			return -1;
		}
		else if (*(unsigned char*)string1 > *(unsigned char*)string2)
		{
			return 1;
		}
		else
		{
			++string1;
			++string2;
		}
	}
	if (*string1 != '\0')
		return 1;
	else if (*string2 != '\0')
		return -1;
	else return 0;
}

int main()
{
	char string1[10] = "abcdef";
	char string2[10] = "ab12";
	printf("%d\n", my_strcmp(string1, string2));
	system("pause");
	return 0;
}
