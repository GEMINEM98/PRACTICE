#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
char a[100];
char b[20];
int main()
{
	int n, i, j = 0, k;
	gets(a);
	n = strlen(a);
	for (i = n - 1; i >= 0; i--)
	{
		if ((a[i] >= 'a'&&a[i] <= 'z') || (a[i] >= 'A'&&a[i] <= 'Z'))
		{
			b[j] = a[i];
			j++;
		}
		else if (j != 0)
		{
			for (k = j - 1; k >= 0; k--)
				printf("%c", b[k]);
			printf(" ");
			j = 0;
		}
	}
	if (j != 0)
	{
		for (k = j - 1; k >= 0; k--)
			printf("%c", b[k]);
		printf(" ");
		j = 0;
	}
	system("pause");
	return 0;
}
