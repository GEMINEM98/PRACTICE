// 一年中的第几天

#include<stdio.h>
int main()
{
	int year,month,day,i,sum=0;
	scanf("%d-%d-%d",&year,&month,&day);	
	if(month==1)
	{
		sum=day;
		printf("%d\n",sum);
	}
	else
	{
	    for(i=1;i<month;++i)
	    {
		    switch(i)
		    {
			    case 2:
				    if((year%4==0&&year%100!=0)||year%400==0)
				    {
				    	sum+=29;
			    	}
				    else
				    {
				    	sum+=28;
				    }
				    continue;
    		    case 1:
			    case 3:
			    case 5:
				case 7:
				case 8:
				case 10:
				case 12:
				    sum+=31;
				    continue;
				case 4:
				case 6:
				case 9:
				case 11:
				   	sum+=30;
				   	continue;
		    }
		}
		printf("%d\n",sum+day);
	}
	return 0;
}
